{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style": "off",
        "semi": 0,
        "no-unused-vars": 0,
        "no-param-reassign": 0,
        "consistent-return": 0
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "document": false
    }
}
